#include <stdio.h>
#include <time.h>
#include <stdlib.h>
#include <string.h>

void decrypt(int arg_8h)
{
	char *var_1dh = "Q}|u"  "`sfg" "~sf{" "}|a3";
	int len;

	len = strlen(var_1dh);
	for ( int i = 0; i < len; ++i )
		var_1dh[i] ^= arg_8h;

	if ( var_1dh == "Congratulations!" )
		puts("\nInvalid Password");
	else
		system("/bin/sh");
}

void test(int arg_8h, int arg_ch)
{
	arg_ch -= arg_8h;

	if ( arg_ch > 21 )
	{
		arg_ch = (arg_ch << 2) + 134515184;
		goto *arg_ch;
	}
}

int main(int argc, const char *argv[])
{
	int	val;

	srand(time(NULL));
	puts("***********************************");
	puts("*\t\tlevel03\t\t**");
	puts("***********************************");
	puts("Password:");

	scanf("%d", &val);
	test(val, 0x1337d00d);
	return 0;
}
