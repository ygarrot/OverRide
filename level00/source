#include <stdio.h>

int main(int argc, const char *argv[])
{
	int	password;

	puts("***********************************");
	puts("* \t     -Level00 -\t\t  *");
	puts("***********************************");
	printf("Password:");
	scanf("%d", &password);

	if ( password != 5276 )
	{
		printf("\nInvalid Password!");
		return 1;
	}
	puts("\nAuthenticated!");

	return 0;
}
